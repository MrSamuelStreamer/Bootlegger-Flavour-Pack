<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <BiomeDef Name="MSSBL_City">
        <defName>MSSBL_City</defName>
        <label>City Block</label>
        <description>A city block</description>
        <workerClass>MSSBL.BiomeWorker_City</workerClass>
        <allowFarmingCamps>false</allowFarmingCamps>
        <animalDensity>0</animalDensity>
        <plantDensity>0</plantDensity>
        <settlementSelectionWeight>0.9</settlementSelectionWeight>
        <movementDifficulty>0.5</movementDifficulty>
        <forageability>0.05</forageability>
        <foragedFood>MealSurvivalPack</foragedFood>
        <wildPlantRegrowDays>25</wildPlantRegrowDays>
        <lakeBeachTerrain>Sand</lakeBeachTerrain>
        <mudTerrain>Marsh</mudTerrain>
        <riverbankTerrain>Riverbank</riverbankTerrain>
        <riverbankSizeRange>6~8</riverbankSizeRange>
        <texture>World/Biomes/MSSBL_City3</texture>
        <maxFishPopulation>500</maxFishPopulation>
        <fishTypes MayRequire="Ludeon.RimWorld.Odyssey">
            <freshwater_Common>
                <Fish_Salmon MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Salmon>
            </freshwater_Common>
            <freshwater_Uncommon>
                <Fish_Frostfish MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Frostfish>
            </freshwater_Uncommon>
            <saltwater_Common>
                <Fish_Salmon MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Salmon>
                <Fish_Cod MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Cod>
            </saltwater_Common>
            <saltwater_Uncommon>
                <Fish_Dogfish MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Dogfish>
            </saltwater_Uncommon>
            <rareCatchesSetMaker MayRequire="Ludeon.RimWorld.Odyssey">RareFishingCatches_Cold</rareCatchesSetMaker>
        </fishTypes>
        <soundsAmbient>
            <li>Ambient_NightInsects_Standard</li>
        </soundsAmbient>
        <diseaseMtbDays>60</diseaseMtbDays>
        <diseases>
            <li>
                <diseaseInc>Disease_Flu</diseaseInc>
                <commonality>100</commonality>
            </li>
            <li>
                <diseaseInc>Disease_Plague</diseaseInc>
                <commonality>100</commonality>
            </li>
            <li>
                <diseaseInc>Disease_GutWorms</diseaseInc>
                <commonality>50</commonality>
            </li>
            <li>
                <diseaseInc>Disease_FibrousMechanites</diseaseInc>
                <commonality>30</commonality>
            </li>
            <li>
                <diseaseInc>Disease_SensoryMechanites</diseaseInc>
                <commonality>30</commonality>
            </li>
            <li>
                <diseaseInc>Disease_MuscleParasites</diseaseInc>
                <commonality>50</commonality>
            </li>
            <li>
                <diseaseInc>Disease_AnimalFlu</diseaseInc>
                <commonality>100</commonality>
            </li>
            <li>
                <diseaseInc>Disease_AnimalPlague</diseaseInc>
                <commonality>100</commonality>
            </li>
            <li>
                <diseaseInc>Disease_OrganDecay</diseaseInc>
                <commonality>10</commonality>
            </li>
        </diseases>
        <terrainsByFertility>
            <li>
                <terrain>Sand</terrain>
                <min>-999</min>
                <max>0.45</max>
            </li>
            <li>
                <terrain>Soil</terrain>
                <min>0.45</min>
                <max>0.90</max>
            </li>
            <li>
                <terrain>SoilRich</terrain>
                <min>0.90</min>
                <max>999</max>
            </li>
        </terrainsByFertility>
        <extraGenSteps>
            <li>City_Walls</li>
            <li>City_Streets</li>
            <!-- <li>City_Bazaars</li> -->
            <li>City_Prison</li>
            <li>City_Hospital</li>
            <li>City_Freezer</li>
            <li>City_MainBuildings</li>
            <li>City_Houses</li>
            <li>City_ThingGroups</li>
            <li>City_Sidewalks</li>
            <li>City_Post</li>
            <li>SettlementPower</li>
            <li>ScatterShrines</li>
            <li>CaveHives</li>
        </extraGenSteps>
        <wildPlants>
            <Plant_Grass>7.0</Plant_Grass>
            <Plant_GrayGrass MayRequire="Ludeon.RimWorld.Biotech">5</Plant_GrayGrass>
            <Plant_Dandelion>0.8</Plant_Dandelion>
            <Plant_Bush>0.7</Plant_Bush>
            <Plant_PincushionCactus>0.6</Plant_PincushionCactus>
            <Plant_Agave>0.2</Plant_Agave>
            <Plant_SaguaroCactus>0.26</Plant_SaguaroCactus>
            <Plant_TreeDrago>0.20</Plant_TreeDrago>
            <Plant_Berry>0.10</Plant_Berry>
            <Plant_PebbleCactus MayRequire="Ludeon.RimWorld.Biotech">0.6</Plant_PebbleCactus>
            <Plant_Reeds MayRequire="Ludeon.RimWorld.Odyssey">1</Plant_Reeds>
        </wildPlants>
        <baseWeatherCommonalities>
            <Clear>18</Clear>
            <Fog>1</Fog>
            <Rain>2</Rain>
            <DryThunderstorm>1</DryThunderstorm>
            <RainyThunderstorm>1</RainyThunderstorm>
            <FoggyRain>1</FoggyRain>
            <SnowGentle>4</SnowGentle>
            <SnowHard>4</SnowHard>
            <GrayPall MayRequire="Ludeon.RimWorld.Anomaly">1</GrayPall>
            <Overcast MayRequire="Ludeon.RimWorld.Odyssey">2</Overcast>
            <Blizzard MayRequire="Ludeon.RimWorld.Odyssey">1</Blizzard>
            <TorrentialRain MayRequire="Ludeon.RimWorld.Odyssey">0.5</TorrentialRain>
        </baseWeatherCommonalities>
        <allowedPackAnimals>
            <li>Muffalo</li>
        </allowedPackAnimals>
    </BiomeDef>


    <BiomeDef ParentName="MSSBL_City">
        <defName>MSSBL_City_PlayerHome</defName>
        <label>City Block</label>
        <description>A city block</description>
        <workerClass>MSSBL.BiomeWorker_City</workerClass>
        <extraGenSteps Inherit="false">
        </extraGenSteps>
    </BiomeDef>


