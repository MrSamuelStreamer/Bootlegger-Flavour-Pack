            <li>City_Post</li>
            <li>SettlementPower</li>
            <li>ScatterShrines</li>
            <li>CaveHives</li>
        </extraGenSteps>
    </BiomeDef>


    <BiomeDef ParentName="MSSBL_City">
        <defName>MSSBL_City_Field</defName>
        <label>Farming City Block</label>
        <description>A farming city block</description>
        <workerClass>MSSBL.BiomeWorker_City</workerClass>
        <extraGenSteps Inherit="false">
            <li>City_Streets</li>
            <li>City_ThingGroups</li>
            <li>City_Sidewalks</li>
            <li>City_Fields</li>
            <li>City_Orchards</li>
            <li>City_Post</li>
            <li>SettlementPower</li>
            <li>ScatterShrines</li>
            <li>CaveHives</li>
        </extraGenSteps>
    </BiomeDef>
</Defs>
